{
  "name": "@tapjs/mock",
  "version": "0.0.0-22",
  "description": "tap plugin adding t.mockRequire() and t.mockImport()",
  "author": "Isaac Z. Schlueter <i@izs.me> (https://blog.izs.me)",
  "type": "module",
  "keywords": [
    "tapjs plugin"
  ],
  "exports": {
    "./package.json": {
      "import": "./package.json",
      "require": "./package.json"
    },
    ".": {
      "import": {
        "types": "./dist/mjs/index.d.ts",
        "default": "./dist/mjs/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    },
    "./loader": {
      "import": {
        "types": "./dist/mjs/legacy-loader.d.ts",
        "default": "./dist/mjs/legacy-loader.js"
      }
    },
    "./import": {
      "import": {
        "types": "./dist/mjs/import.d.ts",
        "default": "./dist/mjs/import.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "license": "BlueOak-1.0.0",
  "scripts": {
    "prepare": "tsc -p tsconfig/cjs.json && tsc -p tsconfig/esm.json && bash ./scripts/fixup.sh",
    "pretest": "npm run prepare",
    "presnap": "npm run prepare",
    "test": "tap",
    "snap": "tap",
    "format": "prettier --write . --loglevel warn --ignore-path ../../.prettierignore --cache",
    "typedoc": "typedoc --tsconfig tsconfig/esm.json ./src/*.ts"
  },
  "tap": {
    "typecheck": false,
    "coverage-map": "map.js"
  },
  "engines": {
    "node": ">=16"
  },
  "funding": {
    "url": "https://github.com/sponsors/isaacs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tapjs/tapjs.git"
  },
  "peerDependencies": {
    "@tapjs/core": "0.0.0-21"
  },
  "dependencies": {
    "@tapjs/after": "0.0.0-21",
    "@tapjs/stack": "0.0.0-7",
    "resolve-import": "^1.2.1"
  }
}
